public class CommonUtils {

    /**
     * 字符串为空判断
     * @param str
     * @return
     */
    public static boolean isEmpty(String str) {
        if (str == null || str.length() == 0 || str.equalsIgnoreCase("null")
                || str.isEmpty() || str.equals("")) {
            return true;
        } else {
            return false;
        }
    }
}
